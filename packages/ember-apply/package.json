{
  "name": "ember-apply",
  "version": "1.0.0",
  "description": "automatic integration and configuration from the community",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./test-utils": "./src/test-utils.js"
  },
  "type": "module",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": false,
  "bin": {
    "ember-apply": "src/cli/index.js"
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "publish": "cp ../../README.md ./ && semantic-release -e semantic-release-monorepo",
    "publish:dry-run": "yarn publish --dry-run",
    "build:types": "tsc --build",
    "lint": "yarn lint:js",
    "lint:js": "eslint .",
    "lint:fix": "yarn lint:js --fix"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/core": "^7.16.7",
    "ember-template-recast": "^6.1.2",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "jscodeshift": "^0.13.1",
    "rehype": "^12.0.0",
    "rehype-parse": "^8.0.3",
    "rehype-raw": "^6.1.1",
    "rehype-stringify": "^9.0.2",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^2.1.17",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jscodeshift": "^0.11.3",
    "@types/node": "^17.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-decorator-position": "^4.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.0",
    "semantic-release-monorepo": "^7.0.0",
    "typescript": "^4.5.4"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
